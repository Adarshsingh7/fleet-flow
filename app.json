{
  "expo": {
    "name": "Fleet Flow",
    "slug": "fleet-flow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/buss-bee.png",
    "scheme": "fast-buss",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.adarsh12q0.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/buss-bee.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.adarsh12q0.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/buss-bee.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "91585793-8329-4788-8872-6ee40d516c2e"
      }
    },
    "owner": "adarsh12q0"
  }
}
